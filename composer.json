{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/rastislav-chynoransky/color-extractor.git",
	"no-api": true
    }],
    "require": {
        "laravel/framework": "5.8.*",
        "caseyamcl/phpoaipmh": "dev-master",
        "intervention/image": "dev-master",
        "guzzlehttp/guzzle": "^6.5",
        "fadion/bouncy": "dev-l5",
        "rtconner/laravel-tagging": "~2.1",
        "maatwebsite/excel": "~2.1",
        "rap2hpoutre/laravel-log-viewer": "dev-master",
        "zizaco/entrust": "5.2.x-dev",
        "laravelium/sitemap": "3.1.*",
        "anahkiasen/former": "^4.0",
        "laravelcollective/html": "~5.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "php": ">=7.2.0",
        "graham-campbell/throttle": "^7",
        "mcamara/laravel-localization": "1.2.*",
        "astrotomic/laravel-translatable": "^11.3",
        "predis/predis": "^1.1",
        "doctrine/dbal": "^2.5",
        "league/csv": "^8.0",
        "league/color-extractor": "dev-master",
        "barryvdh/laravel-form-bridge": "^0.4",
        "symfony/templating": "^3.4",
        "spatie/laravel-newsletter": "^4.3",
        "spatie/laravel-medialibrary": "^3.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/general.php",
            "app/Helpers/i18n.php",
            "app/Helpers/laravel-mix.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*",
        "fzaninotto/faker": "^1.7",
        "laravel/browser-kit-testing": "2.*",
        "filp/whoops": "~2.0",
        "mockery/mockery": "^1"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
